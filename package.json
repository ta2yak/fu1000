{
  "name": "sticky",
  "version": "0.0.1",
  "description": "",
  "main": "main.js",
  "scripts": {
    "compile-dev": "SET NODE_ENV=development && webpack",
    "compile": "SET NODE_ENV=production && webpack",
    "start": "electron .",
    "test": "echo \"Warning: no test specified\"",
    "clean": "rm -rf ./publish",
    "clean:osx": "rm -rf ./publish/osx",
    "clean:win": "rm -rf ./publish/win",
    "pack": "npm run pack:osx && npm run pack:win",
    "pack:osx": "npm run compile && electron-packager . \"Sticky\" --out=publish/osx --platform=darwin --arch=x64 --icon=assets/osx/icon.icns --overwrite --ignore=\"publish|src\"",
    "pack:win": "npm run compile && electron-packager . \"Sticky\" --out=publish/win --platform=win32 --arch=ia32 --icon=assets/win/icon.ico --overwrite --ignore=\"publish|src\"",
    "repack": "npm run repack:osx && npm run repack:win",
    "repack:osx": "npm run clean:osx && npm run pack:osx",
    "repack:win": "npm run clean:win && npm run pack:win",
    "archive": "npm run archive:osx && npm run archive:win",
    "archive:osx": "npm run repack:osx && cd \"publish/osx/\" && zip -r \"Sticky-osx.zip\" \"Sticky-darwin-x64\"",
    "archive:win": "npm run repack:win && cd \"publish/win/\" && zip -r \"Sticky-win-x86.zip\" \"Sticky-win32-ia32\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ta2yak/sticky.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ta2yak/sticky/issues"
  },
  "homepage": "https://github.com/ta2yak/sticky#readme",
  "dependencies": {
    "cron": "^1.2.1",
    "electron-settings": "^3.1.1",
    "electron-window-manager": "^1.1.0",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "melanke-watchjs": "^1.3.1",
    "moment": "^2.18.1",
    "node-notifier": "^5.1.2",
    "semantic-ui": "^2.2.10",
    "uuid": "^3.0.1",
    "vue": "^2.3.4",
    "vuex": "^2.3.1",
    "watchjs": "0.0.0",
    "winston": "^2.3.1",
    "winston-loggly-bulk": "^1.4.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-runtime": "^6.23.0",
    "css-loader": "^0.28.4",
    "electron": "^1.7.3",
    "electron-packager": "^8.7.0",
    "electron-prebuilt": "^1.4.13",
    "vue-hot-reload-api": "^2.1.0",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.2.1",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  },
  "build": {
    "appId": "com.github.ta2yak.sticky",
    "mac": {
      "category": ""
    }
  }
}
